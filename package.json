{
  "name": "andromeda",
  "version": "0.45.4",
  "description": "Make music in your browser!",
  "scripts": {
    "build": "vite build src --emptyOutDir --outDir ../dist && touch dist/robots.txt",
    "fmt": "prettier --ignore-unknown --write '**/*'",
    "start": "vite src --open",
    "test": "run-p test:*",
    "test:fmt": "prettier --check --ignore-unknown '**/*'",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/andromeda.git"
  },
  "keywords": [
    "andromeda",
    "music",
    "synth",
    "web",
    "audio",
    "api"
  ],
  "author": "Ben Hall",
  "license": "SEE LICENSE IN /LICENSE",
  "bugs": {
    "url": "https://github.com/benji6/andromeda/issues"
  },
  "homepage": "https://github.com/benji6/andromeda",
  "dependencies": {
    "capitalize": "^2.0.3",
    "prop-types": "^15.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^4.3.1",
    "redux": "^4.1.1",
    "st88": "github:benji6/st88#v0.0.3",
    "typescript": "^4.4.3",
    "virtual-audio-graph": "^0.20.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^4.3.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.2",
    "vite": "^6.1.1",
    "web-audio-test-api": "^0.5.2"
