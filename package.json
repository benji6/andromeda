{
  "name": "andromeda",
  "description": "Make music in your browser!",
  "scripts": {
    "build": "vite build src --emptyOutDir --outDir ../dist && touch dist/robots.txt",
    "fmt": "prettier --ignore-unknown --write '**/*'",
    "start": "vite src --open",
    "test": "run-p test:*",
    "test:audit": "npm audit --audit-level=critical",
    "test:fmt": "prettier --check --ignore-unknown '**/*'",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/andromeda.git"
  },
  "keywords": [
    "andromeda",
    "music",
    "synth",
    "web",
    "audio",
    "api"
  ],
  "author": "Ben Hall",
  "license": "SEE LICENSE IN /LICENSE",
  "bugs": {
    "url": "https://github.com/benji6/andromeda/issues"
  },
  "homepage": "https://github.com/benji6/andromeda",
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.29.0",
    "redux": "^5.0.1",
    "st88": "github:benji6/st88#v0.0.3",
    "virtual-audio-graph": "^1.3.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.2",
    "typescript": "^5.7.3",
    "vite": "^6.2.0"
  }
}
