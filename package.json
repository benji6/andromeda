{
  "name": "andromeda",
  "version": "0.45.4",
  "description": "Make music in your browser!",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:*",
    "build:parcel": "parcel build --no-source-maps src/index.html",
    "build:robots": "touch dist/robots.txt",
    "fmt": "prettier --write '**/*'",
    "start": "parcel src/index.html --open",
    "test": "run-p test:*",
    "test:fmt": "prettier --check '**/*'",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/andromeda.git"
  },
  "keywords": [
    "andromeda",
    "music",
    "synth",
    "web",
    "audio",
    "api"
  ],
  "author": "Ben Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benji6/andromeda/issues"
  },
  "homepage": "https://github.com/benji6/andromeda",
  "browserslist": [
    ">1.25%"
  ],
  "dependencies": {
    "capitalize": "^2.0.3",
    "prop-types": "^15.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router": "^3.2.0",
    "redux": "^4.1.1",
    "st88": "github:benji6/st88#v0.0.3",
    "typescript": "^4.4.3",
    "virtual-audio-graph": "^0.20.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.4.1",
    "web-audio-test-api": "^0.5.2"
  }
}
