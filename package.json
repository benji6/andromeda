{
  "name": "andromeda",
  "version": "0.45.4",
  "description": "Make music in your browser!",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && run-p build:*",
    "build:assets": "cp -r src/assets dist",
    "build:html": "html-minifier --collapse-whitespace -o dist/index.html src/index.html",
    "build:scripts": "NODE_ENV=production browserify -p bundle-collapser/plugin -t babelify src/index.js | sed 's/process.env.NODE_ENV/\\x27production\\x27/g' | uglifyjs -cm --screw-ie8 > dist/index.js",
    "build:styles": "postcss --use postcss-import --use cssnano -o dist/index.css src/index.css",
    "start": "mkdir -p dist && run-p start:*",
    "start:assets": "nodemon --watch src/assets -e glsl,ico,jpg,png,svg,wav --exec yarn build:assets",
    "start:html": "nodemon --watch src/index.html --exec yarn build:html",
    "start:scripts": "watchify -t babelify src/index.js --o dist/index.js",
    "start:serve": "live-server dist",
    "start:styles": "postcss --use postcss-import -w -o dist/index.css src/index.css",
    "test": "run-p test:*",
    "test:unit": "NODE_ENV=test babel-node node_modules/.bin/tape 'src/**/*-spec.js' | faucet",
    "watch-test:unit": "nodemon --exec yarn test:unit",
    "watch-test": "nodemon --exec yarn test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/andromeda.git"
  },
  "keywords": [
    "andromeda",
    "music",
    "synth",
    "web",
    "audio",
    "api"
  ],
  "author": "Ben Hall",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/benji6/andromeda/issues"
  },
  "homepage": "https://github.com/benji6/andromeda",
  "dependencies": {
    "capitalize": "^1.0.0",
    "localforage": "^1.5.5",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "redux": "^3.7.2",
    "redux-action-buffer": "^1.0.0",
    "redux-persist": "^4.10.2",
    "reselect": "^3.0.1",
    "st88": "github:benji6/st88#v0.0.3",
    "virtual-audio-graph": "^0.20.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-react-router-optimize": "^1.0.2",
    "babel-preset-latest": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^14.5.0",
    "bundle-collapser": "^1.3.0",
    "cssnano": "^3.7.1",
    "faucet": "0.0.1",
    "html-minifier": "^3.5.7",
    "live-server": "^1.2.1",
    "nodemon": "^1.12.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^2.5.2",
    "postcss-import": "^9.0.0",
    "tape": "^4.8.0",
    "uglify-js": "^2.7.0",
    "watchify": "^3.5.0",
    "web-audio-test-api": "^0.5.2"
  }
}
